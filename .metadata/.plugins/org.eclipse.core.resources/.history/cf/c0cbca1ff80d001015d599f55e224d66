package com.kh.start.member.model.vo;

import lombok.Value;

/*
 * Value Object
 * V	 O
 * 특징 : 불변성 => setter가 없음.
 * 
 * @Value => 롬복에서 지원해주는 애노테이션
 */

@Value
public class Member {
	
	private Long memberNo;
	private String memberId;
	private String memberPw;
	private String memberName;
	private String ROLE;
	
}
